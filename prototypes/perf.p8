function _init()
	buls={}
	
	for i=1,750 do
	 local mybul={
	  x=rnd(128),
	  y=rnd(128)
	 }
	 add(buls,mybul)
	end
end

function _draw()
	--cls(2)
	map()
	for bul in all(buls) do
	 sspr(8,0,8,8,bul.x,bul.y)
	 --sspr(16,0,4,4,bul.x,bul.y)

	 --spr(1,bul.x,bul.y)
	end
end

function _update60()
	
	for bul in all(buls) do
	 bul.y+=0.5
	 if bul.y>128 then
	  bul.y-=128
	 end
	end
end
