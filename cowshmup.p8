function die()
 freeze=30
 sfx(2)
end

function die2()
 explode(px-xscroll,py)
 inviz=30
 invul=120
end

function spawnen(eni,enx,eny)
 local en=enlib[eni]
 
 add(enemies,{
  x=enx,
  y=eny,
 
  --x=10+rnd(128),
  --y=16-rnd(32),

  ani=anilib[en[1]],
  anis=en[2],
  sx=0,
  sy=0,
  brain=en[3],
  age=0,
  flash=0,
  hp=en[4],
  col=en[5]
 })

 --[[add(buls,{
		x=90,
		y=64,
		sx=0,
		sy=0,
		ani=anilib[6],
		age=1,
		col=1
	})]]--
	
end

function doenemies()
 for e in all(enemies) do
  
  if e.brain==1 then
   -- flyin and out brain
   if e.age<15 then
    -- fly down
    e.sy=2
   elseif e.age<120 then
    if e.age==60 then
     add(buls,{
  				x=e.x,
  				y=e.y,
  				sx=0,
  				sy=2,
  				ani={22},
  				age=1,
  				col=22
 				})
    end
    -- stay
    e.sy=max(0,e.sy-0.03)
    --e.sy=0
   else
    -- fly up
    e.sy-=0.04
    if e.y < -16 then
     del(enemies,e)
    end
   end
  end
  
  e.x+=e.sx
  e.y+=e.sy
  
  e.age+=1
 end
end

function dobuls(arr)
 for s in all(arr) do
  s.age+=1
  s.x+=s.sx
  s.y+=s.sy
    
  if s.y<-16 or s.y>130 then
   del(arr,s)
  end
 end
end


