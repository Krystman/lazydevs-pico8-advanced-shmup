function upd_game()

 --scrolling
 scroll+=0.2
 
 if #cursegs<1 or scroll>cursegs[#cursegs].o then
  if boss then
   scroll-=64
   for seg in all(cursegs) do
    seg.o-=64
   end
  else
   mapsegi+=1  
  end
  
  local segnum=mapsegs[mapsegi]

  add(cursegs,{
   x=segnum\4*18,
   y=segnum%4*8,
   o=#cursegs<1 and -64 or cursegs[#cursegs].o+64
  })
  
  --‚òÖ
  if #cursegs>2 and scroll-cursegs[1].o>=128 then
   deli(cursegs,1)
  end
 
 end
 
 --movement
 local dir=butarr[1+btn()&0b1111]
 
 if lastdir!=dir and dir>=5 then
  --anti-cobblestone
  px=flr(px)+0.5
  py=flr(py)+0.5
 end
  
 px+=dirx[dir]*spd
 py+=diry[dir]*spd
 local dshipspr=mysgn(dirx[dir])
  
 shipspr+=mysgn(dshipspr-shipspr)*0.15
 shipspr=mid(-1,shipspr,1)
 
 lastdir=dir
 
 --xscroll=mid(0,(px-10)/100,1)*-16
 --2441
 xscroll=mid(0,(px-10)/108,1)\-0.0625
 
 if shotwait>0 then
  shotwait-=1
 else 
  if btn(‚ùé) then
   shoot() 
  end
 end
 
 if btnp(üÖæÔ∏è) then
  explode(64,64)
 end
 
 boss=btn(üÖæÔ∏è)
 
 if t%60==0 then
  spawnen()
 end
 
 dobuls(shots)
 dobuls(buls)
 domuzz()
 doenemies()
 
 pcol=false
 -- shots vs enemies
 for e in all(enemies) do
  e.iscol=false 
 end
 for e in all(enemies) do
  for s in all(shots) do
   if col(flr(xscroll+s.x-3),flr(s.y),8,16,
          flr(xscroll+e.x-7),flr(e.y-7),16,16) then
    e.flash=2
    del(shots,s)
    add(splash,{
     x=s.x,
     y=s.y+4,  
		   sani={23,23,24,25,26},
		   si=0    
    })
    e.hp-=1
    
    if e.hp<=0 then
     del(enemies,e)
     explode(e.x,e.y)
    end 
   end
  end
 end  
 -- ship vs enemies
 for e in all(enemies) do
  if col(flr(px-7),flr(py-7),16,16,
         flr(xscroll+e.x-7),flr(e.y-7),16,16) then
   pcol=true      
  end
 end
 
 -- ship vs bullets
 for b in all(buls) do
  if col(flr(px-7),flr(py-7),16,16,
         flr(xscroll+b.x-3),flr(b.y-3),7,7) then
   pcol=true      
  end
 end
 
 dosplash()
 for p in all(parts) do
  dopart(p)
 end
  
end

